pipeline{
    agent any
     tools { 
        maven 'maven 3.6.3' 
    }
    stages{
        stage('build stage'){
            steps{
                sh 'mvn clean install'
            }

        }
    }
    post {
        always {
            echo 'One way or another, I have finished'
            deleteDir() /* clean up our workspace */
     
        }
    }
}